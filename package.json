{
  "name": "electron-react-ts-starter",
  "version": "1.0.1",
  "description": "A Starter Project for Electron/React/Typescript 2.x",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "build:dev": "webpack --mode development --watch --progress --profile",
    "build:dist": "node_modules/.bin/webpack --config webpack.config.js --mode production --progress --colors",
    "start": "node_modules/.bin/electron .",
    "package:macos": "node_modules/.bin/electron-packager ./ MyApp --platform=darwin --arch=x64 --ignore=src/app --ignore=node_modules --ignore=README.md --ignore=tsconfig.json --ignore=webpack.config.js --ignore=custom_typings --ignore=.vscode --app-version=1.0.0 --icon ./resources/cms_icon.icns",
    "package:win64": "electron-packager ./ MyApp --platform=win32 --arch=x64 --prune=true --ignore=src/app --ignore=node_modules/@types --ignore=node_modules/electron* --app-version=1.0.0 --icon ./resources/myIcon.ico",
    "clean": "cd dist && rm *.js* && cd .."
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^7.10.1",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^0.26.1",
    "electron": "^3.0.8",
    "electron-packager": "^11.1.0",
    "electron-rebuild": "^1.8.2",
    "file-loader": "^0.10.1",
    "json-loader": "^0.5.4",
    "node-loader": "^0.6.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "style-loader": "^0.13.1",
    "typescript": "^3.1.6",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.5.9",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^3.0.0"
  }
}
